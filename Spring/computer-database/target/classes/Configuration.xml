	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/aop
	http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
	http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-2.5.xsd">
	
	<context:annotation-config /> 
	
	<bean name="companyDAO" class="com.computerdatabase.dao.CompanyDAO">
	</bean>
	
	<bean name="daoFactory" class="com.computerdatabase.dao.DAOFactory">
	</bean>
	
	<bean name="computerDAO" class="com.computerdatabase.dao.ComputerDAO">
	</bean>
	
	<bean name="logDAO" class="com.computerdatabase.dao.LogDAO">
	</bean>
	
	<bean name="companyServices" class="com.computerdatabase.service.CompanyServices">
		
	</bean>
	
	<bean name="computerServices" class="com.computerdatabase.service.ComputerServices">
		
	</bean>
</beans>
